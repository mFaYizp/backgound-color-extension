{
    "manifest_version": 3,
    "name":"Background Extension",
    "icons":{
        "32":"icons64.png"
    },
    "version":"1.0.1",
    "background":{
        "service_worker":"service-worker.js",
        "type":"module"
    },
    "action":{
        "default_popup":"index.html"
    },
    "permissions": [
        "scripting"
    ],
    "host_permissions":[
        "http://*/*",
        "https://*/*"
    ]
}